
{
  "type": "component_format",
  "items": [
    "minecraft:wooden_sword",
    "minecraft:raw_iron",
    "minecraft:stick",
    "minecraft:lead",
    "minecraft:debug_stick",
    "minecraft:writable_book",
    "minecraft:firework_rocket",
    "minecraft:candle",
    "minecraft:white_candle",
    "minecraft:orange_candle",
    "minecraft:magenta_candle",
    "minecraft:light_blue_candle",
    "minecraft:yellow_candle",
    "minecraft:lime_candle",
    "minecraft:pink_candle",
    "minecraft:gray_candle",
    "minecraft:light_gray_candle",
    "minecraft:cyan_candle",
    "minecraft:purple_candle",
    "minecraft:blue_candle",
    "minecraft:brown_candle",
    "minecraft:green_candle",
    "minecraft:red_candle",
    "minecraft:black_candle",
    "minecraft:breeze_rod",
    "minecraft:iron_ingot"
  ],
  "modelPrefix": "item/crafting_items/",
  "modelParent": "minecraft:item/handheld",
  "parameters": {
    "format": "${customname}_${itemname}",
    "variables": {
      "customname": {
        "componentType": "custom_name",
        "fallback": "_",
        "transform": ["lowercase",{
          "regex": "(?iu) ",
          "substitution": "_",
          "matchAll": false,
          "validate": false
        },{
          "regex": "(?iu)пиздатенькая_дубинка",
          "substitution": "pizda",
          "matchAll": true,
          "validate": false
        },{
          "regex": "(?iu)резиновый_пенис",
          "substitution": "dildo",
          "matchAll": true,
          "validate": false
        }]

      },
      "itemname": {
        "componentType": "item_name",
        "fallback": "none",
        "transform": ["lowercase",
          {
            "regex": "(?iu)ё",
            "substitution": "е",
            "matchAll": false,
            "validate": false
          },{
            "regex": "(?iu)лупа",
            "substitution": "magnifying_glass",
            "matchAll": true,
            "validate": false
          },
          {
            "regex": "(?iu)корсар",
            "substitution": "firecracker",
            "matchAll": true,
            "validate": false
          },
          {
            "regex": "(?iu)хлопушка",
            "substitution": "firecrecker",
            "matchAll": true,
            "validate": false
          },
          {
            "regex": "(?iu)пыхалка",
            "substitution": "vape",
            "matchAll": true,
            "validate": false
          },
          {
            "regex": "(?iu)бдсм наручники",
            "substitution": "bdsm_kandal",
            "matchAll": true,
            "validate": false
          },
          {
            "regex": "(?iu)полицейская дубинка",
            "substitution": "dubinka",
            "matchAll": true,
            "validate": false
          },
          {
            "regex": "(?iu)разводной ключ",
            "substitution": "pipewrench",
            "matchAll": true,
            "validate": false
          },
          {
            "regex": "(?iu)зонт",
            "substitution": "umbrella",
            "matchAll": true,
            "validate": false
          },
          {
            "regex": "(?iu)свернутый зонт",
            "substitution": "rolled_up_umbrella",
            "matchAll": true,
            "validate": false
          },
          {
            "regex": "(?iu)наручники",
            "substitution": "kandal_icon",
            "matchAll": true,
            "validate": false
          },
          {
            "regex": "(?iu)чертеж",
            "substitution": "blueprint",
            "matchAll": true,
            "validate": false
          },
          {
            "regex": "(?iu)отталкивающая дубинка",
            "substitution": "dubinka_knockback",
            "matchAll": true,
            "validate": false
          },
          {
            "regex": "(?iu)тцк phone",
            "substitution": "tck_phone",
            "matchAll": true,
            "validate": false
          },{
            "regex": "(?iu) ",
            "substitution": "_",
            "matchAll": false,
            "validate": false
          }
        ]
      }
    }
  }
}

