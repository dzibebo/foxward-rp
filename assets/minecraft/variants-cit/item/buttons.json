
{
  "type": "component_data",
  "items": [
    "minecraft:iron_ingot"
  ],
  "modelPrefix": "item/buttons/",
  "modelParent": "minecraft:item/generated",
  "parameters": {
    "componentType": "item_name",
    "expect": "rich_text",
    "debug": true,
    "transform": ["lowercase",
      {
        "regex": "(?iu)ё",
        "substitution": "е",
        "matchAll": false,
        "validate": false
      },
      {
        "regex": "(?iu)кнопка вызовк",
        "substitution": "vizov",
        "matchAll": true,
        "validate": false
      },
      {
        "regex": "(?iu)булава 3d",
        "substitution": "3d_mace_by_pablossska",
        "matchAll": true,
        "validate": false
      },
      {
        "regex": "(?iu)молот судьи",
        "substitution": "molot",
        "matchAll": true,
        "validate": false
      }
    ]
  }
}

